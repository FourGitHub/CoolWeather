[32m+[m[32m                        SharedPreferences.Editor editor = PreferenceManager.getDefaultSharedPreferences(AutoUpdateService.this).edit();[m
[32m+[m[32m                        editor.putString("weather", response);[m
[32m+[m[32m                        editor.apply();[m
[32m+[m[32m                    }[m
